#include <iostream>
using namespace std;

class Date {
private:
    int day;
    int month;
    int year;

public:
    //  constructor for initalizing for date class
    Date() : day(1), month(1), year(2000) {}

    // Parameterized constructor for giving values
    Date(int d, int m, int y) : day(d), month(m), year(y) {}

    // Overloading the >> operator to input date
    friend istream& operator>>(istream& in, Date& date) {
        cout << "Enter day: ";
        in >> date.day;
        cout << "Enter month: ";
        in >> date.month;
        cout << "Enter year: ";
        in >> date.year;
        return in;
    }

    // Overloading the << operator to output date
    friend ostream& operator<<(ostream& out, const Date& date) {
        out << date.day << "/" << date.month << "/" << date.year;
        return out;
    }

    // Overloading the > operator to compare two dates
    bool operator>(const Date& other) const {
        if (year > other.year)
            return true;
        else if (year == other.year && month > other.month)
            return true;
        else if (year == other.year && month == other.month && day > other.day)
            return true;
        return false;
    }
};

int main() {
    Date date1, date2;

    cout << "Enter the first date:\n";
    cin >> date1;

    cout << "Enter the second date:\n";
    cin >> date2;

    cout << "First date: " << date1 << endl;
    cout << "Second date: " << date2 << endl;

    if (date1 > date2) {
        cout << "The first date is later than the second date." << endl;
    } else {
        cout << "The first date is not later than the second date." << endl;
    }

    return 0;
}
